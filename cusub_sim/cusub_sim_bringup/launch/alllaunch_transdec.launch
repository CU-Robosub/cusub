<launch>

  <!-- Obstacle positions -->
  <arg name="SG_x" default="-1.75"/>
  <arg name="SG_y" default="2.75"/>
  <arg name="SG_z" default="-1.3"/>
  <arg name="SG_yaw" default="2.6"/>

  <arg name="PM_x" default="-1.0"/>
  <arg name="PM_y" default="4.5"/>
  <arg name="PM_z" default="-1"/>
  <arg name="PM_yaw" default="-0.2"/>

  <arg name="BJ_x" default="-1.0"/>
  <arg name="BJ_y" default="2.5"/>
  <arg name="BJ_z" default="-1"/>
  <arg name="BJ_yaw" default="0.78"/>

  <arg name="BT_x" default="-1.75"/>
  <arg name="BT_y" default="3.25"/>
  <arg name="BT_z" default="-1"/>
  <arg name="BT_yaw" default="0.4"/>

  <arg name="TO_x" default="4.0"/>
  <arg name="TO_y" default="0.0"/>
  <arg name="TO_z" default="-0.5"/>
  <arg name="TO_yaw" default="1.57"/>

  <arg name="DR_x" default="2.0"/>
  <arg name="DR_y" default="0.2"/>
  <arg name="DR_z" default="-3.5"/>
  <arg name="DR_yaw" default="-0.75"/>

<!--  <arg name="OC_x" default="-3.6"/>
  <arg name="OC_y" default="2.3"/>
  <arg name="OC_z" default="-3.5"/>
  <arg name="OC_yaw" default="-2.7"/> -->

  <!--  <arg name="CO_x" default="-3.6"/>
  <arg name="CO_y" default="2.3"/>
  <arg name="CO_z" default="-3.5"/>
  <arg name="CO_yaw" default="-2.7"/> -->


<include file="$(find robosub_descriptions)/launch/TRANSDEC.launch"/>

<include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="StartGate"/>
      <arg name="x" default="$(arg SG_x)"/>
      <arg name="y" default="$(arg SG_y)"/>
      <arg name="z" default="$(arg SG_z)"/>
      <arg name="yaw" default="$(arg SG_yaw)"/>
  </include>

<include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="PathMarker"/>
      <arg name="x" default="$(arg PM_x)"/>
      <arg name="y" default="$(arg PM_y)"/>
      <arg name="z" default="$(arg PM_z)"/>
      <arg name="yaw" default="$(arg PM_yaw)"/>
  </include>

  <include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="buoy_jiangshi"/>
      <arg name="x" default="$(arg BJ_x)"/>
      <arg name="y" default="$(arg BJ_y)"/>
      <arg name="z" default="$(arg BJ_z)"/>
      <arg name="yaw" default="$(arg BJ_yaw)"/>
  </include>

  <include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="triangular_buoy"/>
      <arg name="x" default="$(arg BT_x)"/>
      <arg name="y" default="$(arg BT_y)"/>
      <arg name="z" default="$(arg BT_z)"/>
      <arg name="yaw" default="$(arg BT_yaw)"/>
  </include>

  <include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="dropper_garlic"/>
      <arg name="x" default="$(arg DR_x)"/>
      <arg name="y" default="$(arg DR_y)"/>
      <arg name="z" default="$(arg DR_z)"/>
      <arg name="yaw" default="$(arg DR_yaw)"/>
  </include>

  <!--<include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="vampire_stake"/>
      <arg name="x" default="$(arg TO_x)"/>
      <arg name="y" default="$(arg TO_y)"/>
      <arg name="z" default="$(arg TO_z)"/>
      <arg name="yaw" default="$(arg TO_yaw)"/>
  </include> -->

    <!--  <include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="octagon"/>
      <arg name="x" default="$(arg OC_x)"/>
      <arg name="y" default="$(arg OC_y)"/>
      <arg name="z" default="$(arg OC_z)"/>
      <arg name="yaw" default="$(arg OC_yaw)"/>
  </include>-->

<!--  <include file="$(find robosub_descriptions)/launch/upload.launch">
      <arg name="obstacle" default="coffin"/>
      <arg name="x" default="$(arg CO_x)"/>
      <arg name="y" default="$(arg CO_y)"/>
      <arg name="z" default="$(arg CO_z)"/>
      <arg name="yaw" default="$(arg CO_yaw)"/>
  </include> -->

  <node name="publish_world_models"
        pkg="uuv_assistants"
        type="publish_world_models.py"
        output="screen">
        <rosparam subst_value="true">
            meshes:
              StartGate_1:
                mesh: package://robosub_descriptions/models/StartGate/StartGate.dae
                pose:
                  position: [$(arg SG_x), $(arg SG_y), $(arg SG_z)]
                  orientation: [0, 0, $(arg SG_yaw)]

              PathMarker_1:
                mesh: package://robosub_descriptions/models/PathMarker/PathMarker.dae
                pose:
                  position: [$(arg PM_x), $(arg PM_y), $(arg PM_z)]
                  orientation: [1.57, 0, $(arg PM_yaw)]

              Jiangshi_1:
                mesh: package://robosub_descriptions/models/buoy_jiangshi/buoy.dae
                pose:
                  position: [$(arg BJ_x), $(arg BJ_y), $(arg BJ_z)]
                  orientation: [0, 0, $(arg BJ_yaw)]

              TriangleBuoy_1:
                mesh: package://robosub_descriptions/models/triangular_buoy/triangular_buoy.dae
                pose:
                  position: [$(arg BT_x), $(arg BT_y), $(arg BT_z)]
                  orientation: [0, 0, $(arg BT_yaw)]

              DropperGarlic_1:
                mesh: package://robosub_descriptions/models/dropper_garlic/dropper_garlic.dae
                pose:
                  position: [$(arg DR_x), $(arg DR_y), $(arg DR_z)]
                  orientation: [1.57, 0, $(arg DR_yaw)]

              <!--vampire_stake_1:
                mesh: package://robosub_descriptions/models/vampire_stake/vampire_stake.dae
                pose:
                  position: [$(arg DR_x), $(arg TO_y), $(arg TO_z)]
                  orientation: [1.57, 0, $(arg TO_yaw)] -->

              <!-- Octagon_1:
                mesh: package://robosub_descriptions/models/Octagon/octagon.dae
                pose:
                  position: [$(arg OC_x), $(arg OC_y), $(arg OC_z)]
                  orientation: [1.57, 0, $(arg OC_yaw)]
              coffin_1:
                mesh: package://robosub_descriptions/models/coffin/coffin.dae
                pose:
                  position: [$(arg CO_x), $(arg CO_y), $(arg CO_z)]
                  orientation: [1.57, 0, $(arg CO_yaw)] -->
        </rosparam>
  </node>

</launch>
