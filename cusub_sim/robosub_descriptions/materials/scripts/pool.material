material Robosub/PoolWall
{
  receive_shadows on
  technique
  {
    pass
    {
       ambient 0.5 0.5 0.9 1.0
       diffuse 0.5 0.5 0.9 1.0
       specular 1 1 1 1 20
       emissive 0.0 0.0 0.0 1.0

    }
  }
}

