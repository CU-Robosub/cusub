material Robosub/Dice5
{
  receive_shadows on
  technique
  {
    pass
    {
      texture_unit
      {
        // Relative to the location of the material script
        texture ../../../textures/5dice.jpg
        // Repeat the texture over the surface (4 per face)
        scale 1.0 1.0
      }
    }
  }
}
