<launch>

    <node pkg="viso2_ros" type="stereo_odometer" name="stereo_odometer" output="screen">

        <param name="image" value=""/>

        <remap from="/stereo/left/image" to="/leviathan/description/camera_8/image_raw_adj"/>
        <remap from="/stereo/right/image" to="/leviathan/description/camera_9/image_raw_adj"/>
        <remap from="/stereo/left/camera_info" to="/leviathan/description/camera_8/camera_info_adj"/>
        <remap from="/stereo/right/camera_info" to="/leviathan/description/camera_9/camera_info_adj"/>

        <param name="max_features" value="10" />
        <param name="motion_threshold" value="100" />
        <param name="camera_height" value="1.12" />
        <param name="camera_pitch"  value="0.00" />

        <param name="base_link_frame_id" value="leviathan/description/base_link"/>
        <param name="publish_tf" value="true"/>
        <param name="odom_frame_id" value="leviathan/description/odom"/>

    </node>

</launch>