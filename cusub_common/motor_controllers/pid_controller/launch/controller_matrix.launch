<launch>

	<arg name="setpoint" default="true" />

	<node name="pose_pid" pkg="pid_controller" type="Pose_PID.py" output="screen"/>
	<node name="pid_pololu" pkg="pid_controller" type="PID_Pololu_matrix.py" output="screen"/>

	<node pkg="topic_tools" type="mux" name="depth_mux" args="local_control/mux/depth/control_effort local_control/pid/depth/control_effort local_control/cv/depth/control_effort">
		<remap from="mux" to="depth_mux"/>
		<param name="initial_topic " value="local_control/pid/depth/control_effort" />
	</node>
	<node name="depth_pid" pkg="pid" type="controller" >
		<param name="node_name" value="depth_pid" />
		<param name="topic_from_controller" value="local_control/pid/depth/control_effort" />
		<param name="topic_from_plant" value="local_control/pid/depth/state" />
		<param name="setpoint_topic" value="local_control/pid/depth/setpoint" />
		<param name="Kp" value="-100.0" />
		<param name="Ki" value="0.0" />
		<param name="Kd" value="-50.0" />
		<param name="rate" value="25" />
		<param name="upper_limit" value="100" />
		<param name="lower_limit" value="-100" />
		<param name="windup_limit" value=".001" />
		<param name="diagnostic_period" value="0.25" />
		<param name="max_loop_frequency" value="25.0" />
		<param name="min_loop_frequency" value="25.0" />
	</node>
	<node name="depth_cv" pkg="bangbang" type="bangbang.py">
		<param name="setpoint_topic" value="local_control/cv/depth/setpoint" />
		<param name="state_topic" value="local_control/cv/depth/state" />
		<param name="control_effort_topic" value="local_control/cv/depth/control_effort" />
		<param name="deadzone_size" value="0" />
		<param name="left_bang_effort" value="65" />
		<param name="right_bang_effort" value="75" />
	</node>

	<group if="$(arg setpoint)">

		<node name="pitch_pid" pkg="pid" type="controller" >
			<param name="node_name" value="pitch_pid" />
			<param name="topic_from_controller" value="local_control/pid/pitch/control_effort" />
			<param name="topic_from_plant" value="local_control/pid/pitch/state" />
			<param name="setpoint_topic" value="local_control/pid/pitch/setpoint" />
			<param name="Kp" value="20.0" />
			<param name="Ki" value="0.0" />
			<param name="Kd" value="0.0" />
			<param name="rate" value="25" />
			<param name="upper_limit" value="100" />
			<param name="lower_limit" value="-100" />
			<param name="windup_limit" value=".001" />
			<param name="diagnostic_period" value="0.25" />
			<param name="max_loop_frequency" value="25.0" />
			<param name="min_loop_frequency" value="25.0" />
		</node>
		<node name="roll_pid" pkg="pid" type="controller" >
			<param name="node_name" value="roll_pid" />
			<param name="topic_from_controller" value="local_control/pid/roll/control_effort" />
			<param name="topic_from_plant" value="local_control/pid/roll/state" />
			<param name="setpoint_topic" value="local_control/pid/roll/setpoint" />
			<param name="Kp" value="20.0" />
			<param name="Ki" value="0.0" />
			<param name="Kd" value="0.0" />
			<param name="rate" value="25" />
			<param name="upper_limit" value="100" />
			<param name="lower_limit" value="-100" />
			<param name="windup_limit" value=".001" />
			<param name="diagnostic_period" value="0.25" />
			<param name="max_loop_frequency" value="25.0" />
			<param name="min_loop_frequency" value="25.0" />
		</node>
		<node pkg="topic_tools" type="mux" name="yaw_mux" args="local_control/mux/yaw/control_effort local_control/pid/yaw/control_effort local_control/cv/yaw/control_effort">
			<remap from="mux" to="yaw_mux"/>
			<param name="initial_topic " value="local_control/pid/yaw/control_effort" />
		</node>
		<node name="yaw_pid" pkg="pid" type="controller" >
			<param name="node_name" value="yaw_pid" />
			<param name="topic_from_controller" value="local_control/pid/yaw/control_effort" />
			<param name="topic_from_plant" value="local_control/pid/yaw/state" />
			<param name="setpoint_topic" value="local_control/pid/yaw/setpoint" />
			<param name="Kp" value="500.0" />
			<param name="Ki" value="250.0" />
			<param name="Kd" value="0.0" />
			<param name="rate" value="25" />
			<param name="upper_limit" value="100" />
			<param name="lower_limit" value="-100" />
			<param name="windup_limit" value=".001" />
			<param name="diagnostic_period" value="0.25" />
			<param name="max_loop_frequency" value="25.0" />
			<param name="min_loop_frequency" value="25.0" />
			<param name="angle_error" value="true" />
		</node>
		<node name="yaw_cv" pkg="bangbang" type="bangbang.py">
			<param name="setpoint_topic" value="local_control/cv/yaw/setpoint" />
			<param name="state_topic" value="local_control/cv/yaw/state" />
			<param name="control_effort_topic" value="local_control/cv/yaw/control_effort" />
			<param name="deadzone_size" value="30" />
			<param name="left_bang_effort" value="40" />
			<param name="right_bang_effort" value="-40" />
		</node>
		<node pkg="topic_tools" type="mux" name="drive_mux" args="local_control/mux/drive/control_effort local_control/pid/drive/control_effort local_control/cv/drive/control_effort">
			<remap from="mux" to="drive_mux"/>
			<param name="initial_topic " value="local_control/pid/drive/control_effort" />
		</node>
		<node name="drive_pid" pkg="pid" type="controller" >
			<param name="node_name" value="drive_pid" />
			<param name="topic_from_controller" value="local_control/pid/drive/control_effort" />
			<param name="topic_from_plant" value="local_control/pid/drive/state" />
			<param name="setpoint_topic" value="local_control/pid/drive/setpoint" />
			<param name="Kp" value="30.0" />
			<param name="Ki" value="0.0" />
			<param name="Kd" value="0.0" />
			<param name="rate" value="25" />
			<param name="upper_limit" value="100" />
			<param name="lower_limit" value="-100" />
			<param name="windup_limit" value=".001" />
			<param name="diagnostic_period" value="0.25" />
			<param name="max_loop_frequency" value="25.0" />
			<param name="min_loop_frequency" value="25.0" />
		</node>
		<node name="drive_cv" pkg="bangbang" type="bangbang.py">
			<param name="setpoint_topic" value="local_control/cv/drive/setpoint" />
			<param name="state_topic" value="local_control/cv/drive/state" />
			<param name="control_effort_topic" value="local_control/cv/drive/control_effort" />
			<param name="deadzone_size" value="30" />
			<param name="left_bang_effort" value="60" />
			<param name="right_bang_effort" value="-60" />
		</node>
		<node pkg="topic_tools" type="mux" name="strafe_mux" args="local_control/mux/strafe/control_effort local_control/pid/strafe/control_effort local_control/cv/strafe/control_effort">
			<remap from="mux" to="strafe_mux"/>
			<param name="initial_topic " value="local_control/pid/strafe/control_effort" />
		</node>
		<node name="strafe_pid" pkg="pid" type="controller" >
			<param name="node_name" value="strafe_pid" />
			<param name="topic_from_controller" value="local_control/pid/strafe/control_effort" />
			<param name="topic_from_plant" value="local_control/pid/strafe/state" />
			<param name="setpoint_topic" value="local_control/pid/strafe/setpoint" />
			<param name="Kp" value="50.0" />
			<param name="Ki" value="0.0" />
			<param name="Kd" value="0.0" />
			<param name="rate" value="25" />
			<param name="upper_limit" value="100" />
			<param name="lower_limit" value="-100" />
			<param name="windup_limit" value=".001" />
			<param name="diagnostic_period" value="0.25" />
			<param name="max_loop_frequency" value="25.0" />
			<param name="min_loop_frequency" value="25.0" />
		</node>
		<node name="strafe_cv" pkg="bangbang" type="bangbang.py">
			<param name="setpoint_topic" value="local_control/cv/strafe/setpoint" />
			<param name="state_topic" value="local_control/cv/strafe/state" />
			<param name="control_effort_topic" value="local_control/cv/strafe/control_effort" />
			<param name="deadzone_size" value="30" />
			<param name="left_bang_effort" value="-100" />
			<param name="right_bang_effort" value="100" />
		</node>

	</group>

</launch>
